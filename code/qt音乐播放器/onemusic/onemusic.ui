/*---------------------------------------------------------------
QTabWidget
-----------------------------------------------------------------*/
QTabWidget::pane{border: 2px solid #FFCC33; background: #535353;}
QTabWidget::pane:top{top:-2px;} 
QTabWidget::pane:bottom{bottom:-2px;} 
QTabWidget::pane:left{left:-2px;} 
QTabWidget::pane:right{right:-2px;}
QTabWidget::pane:disabled{border: 2px solid rgb(201, 201, 201); background: #9F9F9F;}
QTabWidget .QWidget{border:0px;}

QTabBar::tab
{
	background:#383838; 
	color:#FFFFFF; 
	font: 75 11pt; 
	min-height:48px; 
	min-width:100px; 
	margin-left: 1px; 
	margin-right: 1px;
}

QTabBar::tab:selected
{
	background:#535353; 
	margin-bottom:-2px; 
	margin-left: 0px;	
	margin-right: 0px;
	border:2px solid #FFCC33; 
}

QTabBar::tab:disabled
{
	background-color: #9F9F9F;
	color:#DDDDDD;
	border:2px solid rgb(201, 201, 201);
}

QTabBar::tab:first{ margin-left: 0px;}
QTabBar::tab:last{margin-right: 0px;}
QTabBar::tab:!selected{border-bottom: 2px solid #FFCC33; margin-top: 0px;}
QTabBar::tab:!selected:disabled{border-bottom: 2px solid rgb(201, 201, 201);}
QTabBar::scroller{width: 60px;}
 
QTabBar QToolButton 
{ 
	background:#383838;
	color:#FFFFFF;
	border:none;
	border-bottom:2px solid #FFCC33;
	border-radius: 0px;
}

QTabBar QToolButton:pressed
{
	background:#535353;
	color:#FFFFFF;
	border:none;
	border-bottom:2px solid #FFCC33;
	border-radius: 0px;
}

QTabBar QToolButton:hover
{
	background:#535353;
	color:#FFFFFF;
	border:none;
	border-bottom:2px solid #FFCC33;
	border-radius: 0px;
}

QTabBar QToolButton:disabled
{
	background: #9F9F9F;
	color:#DDDDDD;
	border:none;
	border-bottom:2px solid #FFCC33;
	border-radius: 0px;
}

QTabBar QToolButton::left-arrow{image: url(:/Resources/ArrowLeft.png);}
QTabBar QToolButton::right-arrow{image: url(:/Resources/ArrowRight.png);}
 
