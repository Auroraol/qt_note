/*--------------------------------------------------------------- 
cLoggerWidget
--------------------------------------------------------------- */
cLoggerWidget
{
	background-color:#282828; 
    color:#FFFFFF;
}

cLoggerWidget::pane
{
    background-color:#282828; 
    color:#FFFFFF;
    selection-background-color:#282828;
    selection-color:#FFFFFF;
    font-size:14px;
    font-family:'Microsoft YaHei';
}

cLoggerWidget::item:selected,
cLoggerWidget::item:hover
{
	background-color:#FCE6A2;
	padding-left:6px;
	border-bottom:1px solid #f76900;
}

cLoggerWidget::item:alternate:!selected
{ 
    background-color:#282828;
} 

cLoggerWidget::item:!alternate:!selected
{ 
    background-color:#282828;
}

/*--------------------------------------------------------------- 
3VJ CNC Controls
--------------------------------------------------------------- */
CncIOButton,
CncRegisterButton,
CncToggleButton,
CncJogButton,
CncJogMultiButton
{
	background:#686868;
	color:#FFFFFF;
	border:2px solid #505050;
	border-radius:0px;
	font-size:14px;
	font-family:'Microsoft yahei';
}


CncIOButton:hover,
CncRegisterButton:hover,
CncToggleButton:hover,
CncJogButton:hover,
CncJogMultiButton:hover
{
	color:#FFFFFF;
	border-radius:0px;
	border:2px solid #F76900;
}

CncIOButton:pressed,
CncRegisterButton:pressed,
CncToggleButton:pressed,
CncJogButton:pressed,
CncJogMultiButton:pressed
{
	background:#FFCC33;
	color:#4D4D4D;
	border-radius:0px;
	border:2px solid #F76900;
}

CncIOButton:checked,
CncRegisterButton:checked,
CncToggleButton:checked,
CncJogButton:checked,
CncJogMultiButton:checked
{
	background:#FFCC33;
	color:#4D4D4D;
	border-radius:0px;
	border:2px solid #F76900;
}

CncIOButton:disabled,
CncRegisterButton:disabled,
CncToggleButton:disabled,
CncJogButton:disabled,
CncJogMultiButton:disabled
{
	background-color: #9F9F9F;
	color:#DDDDDD;
	border-radius:0px;
}


